<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ParcelScout.Nucleo" assembly="ParcelScout.Nucleo">
  <class name="ParcelScout.Nucleo.Entidades.Destinatario, ParcelScout.Nucleo" table="destinatario" lazy="false">
    <id name="Id" type="int">
      <column name="id"/>
      <generator class="identity"/>
    </id>
    <property name="Nombre" type="string">
      <column name="nombre"/>
    </property>
    <property name="Domicilio" type="string">
      <column name="domicilio"/>
    </property>
    <property name="CodigoPostal" type="string">
      <column name="codigo_postal"/>
    </property>
    <property name="Ciudad" type="string">
      <column name="ciudad"/>
    </property>
    <property name="Estado" type="string">
      <column name="estado"/>
    </property>
    <property name="Telefono" type="string">
      <column name="telefono"/>
    </property>
    <property name="Correo" type="string">
      <column name="correo"/>
    </property>
    <property name="Recibe" type="string">
      <column name="recibe"/>
    </property>
  
  </class>
</hibernate-mapping>
